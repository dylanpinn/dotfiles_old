"*":
  "atom-beautify":
    cs:
      beautify_on_save: true
    css:
      convert_quotes: "single"
      newline_between_rules: true
      no_lead_zero: true
      preserve_newlines: true
      selector_separator_newline: true
    erb:
      default_beautifier: "HTML Beautifier"
      indent_size: 2
    general:
      _analyticsUserId: "99afad7b-7444-488b-a2b6-f6aa0f2f05e0"
    html:
      end_with_newline: true
    js:
      end_with_newline: true
      indent_size: 2
      space_after_anon_function: true
    json:
      end_with_newline: true
      indent_size: 2
    php: {}
    ruby:
      indent_size: 2
    scss:
      convert_quotes: "single"
      indent_size: 2
      newline_between_rules: true
      no_lead_zero: true
      preserve_newlines: true
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "metrics"
      "exception-reporting"
      "linter-phpmd"
      "linter-htmlhint"
      "linter-phpcs"
      "linter-codeclimate"
      "react"
    ]
    hideGitIgnoredFiles: true
    projectHome: "~/dev/"
  editor:
    fontFamily: "Operator Mono"
    fontSize: 15
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
  emmet: {}
  "file-icons":
    coloured: false
    tabPaneIcon: false
  "go-debug":
    limitCommandsToGo: true
    panelInitialVisible: false
  "go-plus":
    goPath: "~/dev/go"
  "go-rename": {}
  gofmt:
    formatTool: "goreturns"
  "language-babel": {}
  linter: {}
  "linter-codeclimate":
    init: false
  "linter-eslint":
    fixOnSave: true
    lintHtmlFiles: true
  "linter-jscs":
    fixOnSave: true
    onlyConfig: true
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
    lintInlineJavaScript: true
  "linter-phpmd":
    projectRules: true
  "linter-rubocop": {}
  "linter-sass-lint":
    noConfigDisable: true
  "linter-scss-lint":
    disableWhenNoConfigFileInPath: true
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "project-manager":
    environmentSpecificProjects: true
  "ruby-test":
    testFramework: "minitest"
  stylefmt:
    formatOnSave: true
  "todo-show":
    ignoreThesePaths: [
      "**/node_modules/"
      "**/vendor/"
      "**/bower_components/"
      "**/public/"
    ]
  "tree-view":
    hideVcsIgnoredFiles: true
  "unity-ui":
    showIcons: true
  wakatime:
    apikey: "Saved in your ~/.wakatime.cfg file"
  "wakatime-hidden":
    lastInit: 1476083007
  welcome:
    showOnStartup: false
